     <servlet>
    <servlet-name>AdminInfoServlet</servlet-name>
    <servlet-class>com.dbDesign.web.controller.AdminInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginUIServlet</servlet-name>
    <url-pattern>/servlet/LoginUIServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrderTicketUIServlet</servlet-name>
    <url-pattern>/servlet/OrderTicketUIServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>GuestInfoUIServlet</servlet-name>
    <url-pattern>/servlet/GuestInfoUIServlet</url-pattern>
